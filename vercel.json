{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/home",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/user",
      "dest": "/routes/user.js"
    },
    {
      "src": "/api/v1/product",
      "dest": "/routes/product.js"
    },
    {
      "src": "/api/v1/payment",
      "dest": "/routes/payment.js"
    },
    {
      "src": "/api/v1/order",
      "dest": "/routes/order.js"
    }
  ]
}
